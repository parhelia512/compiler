(** 
 * 11. Program Units.
 * 11.2. object
 * Object visibility
 *
 * Positive test: the default (public) visibility value is applied for the object A.B.
 * The object A.B is declared after was used.
 *)
module Main;
	import A.B;

	var b : A.B;
begin
	if b.i = 3 
	then halt(1) else halt(0) end;
end Main.

object { public } A.B;
	var { public } 
		i : integer;
begin 
	i := 3; 
end B.