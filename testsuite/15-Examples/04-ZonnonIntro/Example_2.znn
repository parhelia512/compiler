module Example_2;
type 
  Figure = (Star, Rectangle, Circle, Ellipse);
  
var 
	a, b: real;
	x, y: integer;
	myfigure: Figure;
	align: (Top, Bottum, Left, Right);
begin
  align := align.Right;
  writeln(integer(align));
end Example_2.
