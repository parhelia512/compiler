module Sorting;
    procedure Maximum(a,b:integer):integer;
	begin
		if a > b then
			return a
		else
			return b
		end
	end Maximum;	

var a,b:integer;
begin
 write("A = ");	readln(a);
 write("B = ");	readln(b);
 writeln("max( A, B ) = ", Maximum(a,b));
end Sorting.

