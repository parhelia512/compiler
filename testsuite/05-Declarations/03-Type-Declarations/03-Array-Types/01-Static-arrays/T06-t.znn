(** 
 * 5. Declarations.
 * 5.3. Type declaration.
 * 5.3.4. String type.
 *
 * Positive test: 2-dimensional arrays assignment.
 * Long declaration of 2-dimensional array: array 8 of array 8.
 *)
module Main;
	type T = array 8 of array 8 of boolean;
	var x : T;
begin
	x[1][1] := true;

	if x[1][1] 
	then halt(1) else halt(0) end;
end Main.