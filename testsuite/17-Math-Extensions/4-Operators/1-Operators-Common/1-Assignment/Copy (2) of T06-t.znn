(** 
 * 17. Math extensions.
 * 17.4. Operators
 * 17.4.1. Assignment
 * 
 * Negative test: lengths are not equal
 * math static := dynamic
 *)

module Main;

type v = array *, *, * of integer;

var
	a : array {math} 2, 3, 4 of integer;
	b : v;
	
begin	
	b := new v(2, 3, 3);
	do
		a := b;	
	on IncompatibleSizes do
		halt(1);
	end;
	halt(0);
end Main.