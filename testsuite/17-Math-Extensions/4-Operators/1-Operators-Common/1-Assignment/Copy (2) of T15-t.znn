(** 
 * 17. Math extensions.
 * 17.4. Operators
 * 17.4.1. Assignment
 * 
 * Negative test: dynamic array in the right part of assignment was not initialized
 * math dynamic := dynamic
 *)

module Main;

type v = array {math} *, *, * of integer;
type v1 = array *, *, * of integer;

var
	a : v;
	b : v1;
	
begin	
	do
		a := b;	
	on NilReference do
		halt(1);
	end;
	halt(0);
end Main.