(** 
 * 8. Procedure declaration and formal parameters
 *
 * Positive test: the function call uses correct variable parameter.
 *)
module Main;
	procedure A(i : integer) : integer;
	begin
		return i + 1;
	end A;

	var i : integer;
begin
	i := A(i);

	if i = 2 
	then halt(1) else halt(0) end;
end Main.