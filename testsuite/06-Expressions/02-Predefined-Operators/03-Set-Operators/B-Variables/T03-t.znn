(** 
 * 6. Expression.
 * 6.2. Predefined Operators.
 * 6.2.4. Set Operators.
 *
 * Positive test: set union.
 *)
module Main;
	var s1, s2, s3 : set;
begin
	s1 := { 1, 2 };
	s2 := { 2, 3 };
	s3 := s1 + s2; (* { 1, 2, 3 } is content of s3 set. *)

	if ~(0 in s3) & (1 in s3) & (2 in s3) & (3 in s3) & ~(4 in s3)
	then halt(1) else halt(0) end;
end Main.