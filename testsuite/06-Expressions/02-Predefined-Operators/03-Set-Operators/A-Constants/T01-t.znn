(** 
 * 6. Expression.
 * 6.2. Predefined Operators.
 * 6.2.4. Set Operators.
 *
 * Positive test: check the set content.
 * Set is presented as constant.
 *)
module Main;
begin
	if ~(0 in { 1, 2, 3 }) & 
        (1 in { 1, 2, 3 }) & 
		(2 in { 1, 2, 3 }) & 
		(3 in { 1, 2, 3 }) & 
	   ~(4 in { 1, 2, 3 })
	then halt(1) else halt(0) end;
end Main.