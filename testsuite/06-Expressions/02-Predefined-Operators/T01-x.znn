(** 
 * 6. Expression.
 * 6.2. Predefined Operators.
 * 6.2.1. Operator precedence.
 *
 * Negative test: wrong logical expression.
 * The operator or has higher priority then operators => and >=.	
 *)
module Main;
	var i : integer;
begin
	i := 10;
	if  i <= 10 or i >= 10
	then halt(1) else halt(0) end;
end Main.