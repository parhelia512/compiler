(** 
 * 6. Expression.
 * 6.2. Predefined Operators.
 * 6.2.3. Arithmetic Operators
 *
 * Positive test: 5 - 2 with object array elements.
 * The object is declared before its using.
 *)
object A.B;
	var { public }
		x , y : array 2 of integer;
end B.

module Main;
	import A;

	var b : A.B;
begin
	b.x[0] := 5;
	b.y[1] := 3;

	if b.x[0] + b.y[1] = 8 
	then halt(1) else halt(0) end;
end Main.
