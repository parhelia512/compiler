(** 
 * 6. Expression.
 * 6.2. Predefined Operators.
 * 6.2.2. Logical Operators.
 *
 * Negative test: private procedures with boolean result are used in logical operator.
 * The object is declared before its procedures are used.
 *)
object A.B;
	procedure { private } p : boolean;
	begin 
		return true 
	end p;

	procedure { private } q : boolean;
	begin 
		return false 
	end q;
end B.

module Main;
	import A;

	var b : A.B;
		r : boolean;
begin
	r := b.p or b.q;
	halt(0);
end Main.