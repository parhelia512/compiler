(** 
 * 7. Statement 
 * 7.2. Procedure call. 
 *
 * Negative test: wrong parameters count in procedure call.
 *)
object {public} A.B;
	procedure {public} p(var j : integer; i : integer);
	begin  
		j := i + 1;
	end p;
end B.

module Main;
	import A.B;

	var p1, p2 : integer;

	procedure { public } rp : A.B;
		var bb : A.B;
	begin  
		return bb;
	end rp;
begin
	p1 := 1;
	p2 := 2;
	rp.p(p1, p2);

	if p1 = 3 
	then halt(1) else halt(0) end;
end Main.
